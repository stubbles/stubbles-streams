{
    "name": "stubbles/streams",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Input- and OutputStreams.",
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "mikey179/vfsStream": "^1.6",
        "bovigo/assert": "^2.1",
        "bovigo/callmap": "3.2",
        "stubbles/reflect": "^8.0",
        "stubbles/sequence": "^8.1",
        "ext-iconv": "*"
    },
    "suggest": {
        "stubbles/sequence": "Easer iteration of input streams with lines() function.",
        "ext-iconv": "For use in decoding and encoding stream decorators."
    },
    "autoload": {
        "psr-4": {
            "stubbles\\streams\\": "src/main/php"
        },
        "files": ["src/main/php/functions.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
